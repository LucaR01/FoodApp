<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Include the rectangle_background shape as the background -->
    <item android:drawable="@drawable/rectangle_background" />

    <!-- Add your custom green color with rounded corners on top of the background -->
    <item android:left="1dp" android:right="1dp" android:top="1dp" android:bottom="1dp">
        <shape android:shape="rectangle">
            <corners android:radius="30dp" />
            <solid android:color="@color/custom_green" />
        </shape>
    </item>
</layer-list>